{
  "projects": {
    "default": "adm-web-pro"
  },
  "targets": {},
  "etags": {},
  "version": "1.0.0",
  "firestore": {
    "rules": "firestore.rules"
  },
  "hosting": [
    {
      "target": "web",
      "public": "web",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ],
      "headers": [
        {
          "source": "**/*.@(js|css|svg)",
          "headers": [
            {
              "key": "Cache-Control",
              "value": "public, max-age=1year"
            }
          ]
        },
        {
          "source": "index.html",
          "headers": [
            {
              "key": "Cache-Control",
              "value": "public, max-age=0"
            }
          ]
        }
      ],
      "rewrites": [
        {
          "source": "**",
          "destination": "/index.html"
        }
      ]
    }
  ]
}
